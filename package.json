{
  "name": "api-node-express",
  "version": "1.0.0",
  "description": "API Node express",
  "main": "server.js",
  "scripts": {
    "start": "node ./src/server.js",
    "dev": "nodemon ./src/server.js",
    "test": "npm run test:unit -- --silent=true; npm run test:integration -- --silent=true",
    "test:integration": "jest tests/integration",
    "test:unit": "jest tests/unit",
    "test:coverage": "jest --coverage",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:migrate:undo:all; npm run db:migrate; npm run db:seed"
  },
  "keywords": [
    "api",
    "node",
    "express"
  ],
  "author": "UÃ©lio Nobre",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.2",
    "sequelize": "^6.37.1"
  },
  "devDependencies": {
    "chai": "^4.4.1",
    "chai-http": "^4.4.0",
    "jest": "^29.7.0",
    "mocha": "^10.3.0",
    "nodemon": "^3.1.0",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0"
  }
}
